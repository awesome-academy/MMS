<% if @team.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= t "form_contains" %> <%= pluralize @team.errors.count, "error" %>.
    </div>
    <ul>
    <% @team.errors.full_messages.each do |msg| %>
      <li>
        <%= msg %>
      </li>
    <% end %>
    </ul>
  </div>
<% end %>
